package JavaAdvanced.InterfaceEx.generic.practice;

public class MyArrayListPractice {
	private Object[] nums;
	private int current;
	public MyArrayListPractice() {
		nums = new Object[5];
		current = 0;
	}
	public void add(Object obj) {
		nums[current] = obj;
		current++;
	}
	public Object get(int index) {
		return nums[index];
	}
	public int size() {
		return current;
	}
	
	public void clear() {
		current=0;
	}
}
